<?php

namespace BackendBundle\Entity;

/**
 * TblPlato
 */
class TblPlato
{
    /**
     * @var string
     */
    private $codPlato;

    /**
     * @var string
     */
    private $nombrePlato;

    /**
     * @var string
     */
    private $descripcionPlato;

    /**
     * @var integer
     */
    private $idTipoPlato;

    /**
     * @var integer
     */
    private $estrellas = '1';

    /**
     * @var \BackendBundle\Entity\TblTipoPlato
     */
    private $idPlato;


    /**
     * Set codPlato
     *
     * @param string $codPlato
     *
     * @return TblPlato
     */
    public function setCodPlato($codPlato)
    {
        $this->codPlato = $codPlato;

        return $this;
    }

    /**
     * Get codPlato
     *
     * @return string
     */
    public function getCodPlato()
    {
        return $this->codPlato;
    }

    /**
     * Set nombrePlato
     *
     * @param string $nombrePlato
     *
     * @return TblPlato
     */
    public function setNombrePlato($nombrePlato)
    {
        $this->nombrePlato = $nombrePlato;

        return $this;
    }

    /**
     * Get nombrePlato
     *
     * @return string
     */
    public function getNombrePlato()
    {
        return $this->nombrePlato;
    }

    /**
     * Set descripcionPlato
     *
     * @param string $descripcionPlato
     *
     * @return TblPlato
     */
    public function setDescripcionPlato($descripcionPlato)
    {
        $this->descripcionPlato = $descripcionPlato;

        return $this;
    }

    /**
     * Get descripcionPlato
     *
     * @return string
     */
    public function getDescripcionPlato()
    {
        return $this->descripcionPlato;
    }

    /**
     * Set idTipoPlato
     *
     * @param integer $idTipoPlato
     *
     * @return TblPlato
     */
    public function setIdTipoPlato($idTipoPlato)
    {
        $this->idTipoPlato = $idTipoPlato;

        return $this;
    }

    /**
     * Get idTipoPlato
     *
     * @return integer
     */
    public function getIdTipoPlato()
    {
        return $this->idTipoPlato;
    }

    /**
     * Set estrellas
     *
     * @param integer $estrellas
     *
     * @return TblPlato
     */
    public function setEstrellas($estrellas)
    {
        $this->estrellas = $estrellas;

        return $this;
    }

    /**
     * Get estrellas
     *
     * @return integer
     */
    public function getEstrellas()
    {
        return $this->estrellas;
    }

    /**
     * Set idPlato
     *
     * @param \BackendBundle\Entity\TblTipoPlato $idPlato
     *
     * @return TblPlato
     */
    public function setIdPlato(\BackendBundle\Entity\TblTipoPlato $idPlato = null)
    {
        $this->idPlato = $idPlato;

        return $this;
    }

    /**
     * Get idPlato
     *
     * @return \BackendBundle\Entity\TblTipoPlato
     */
    public function getIdPlato()
    {
        return $this->idPlato;
    }
}

